{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "allowJs": true,
    "outDir": "./dist",
    "baseUrl": "./src"
  },
  "include": [
    "src/**/*", "../gui/src/components/injects/injects-view.ts", "../gui/src/components/injects/injects-list.ts", "../gui/src/components/injects/injects-form.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
